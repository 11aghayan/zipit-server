{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS"],
      "status": 200,
      "headers": {
        "Access-Control-Allow-Origin": "https://zipit-admin.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}